<h1>Yearbook</h1>
<h1>Lista de usuários</h1>

<table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Senha</th>
      <th>Foto</th>
      <th>Email</th>
      <th>Nome</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>String</th>
      <th>Descrição</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.Login %></td>
        <td><%= usuario.Senha %></td>
        <td><%= usuario.Foto %></td>
        <td><%= usuario.Email %></td>
        <td><%= usuario.Nome %></td>
        <td><%= usuario.Cidade %></td>
        <td><%= usuario.Estado %></td>
        <td><%= usuario.Descrição %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
